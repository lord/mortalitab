{
  "manifest_version": 2,
  "name": "Mortalitab",
  "version": "1.3",
  "description": "Replace your new tab page with a personal reminder that you will die.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "options_page": "settings.html",
  "icons": {
    "48": "assets/48.png",
    "128": "assets/128.png"
  },
  "permissions": [
    "history"
  ]
}